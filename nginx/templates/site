server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    ssl_dhparam /etc/ssl/certs/dhparam.pem;

    server_name %(server_name)s;

    client_max_body_size 100M;

    location /static/ {
        alias /code/static;
    }

    location /media/ {
        alias /code/media;
    }

    location / {
        include proxy_params;
        proxy_pass http://website;
    }
}

